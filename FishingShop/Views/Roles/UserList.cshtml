@model IEnumerable<FishingShop.Models.ApplicationUser>
<h2>Users</h2>
<table class="table">
	@foreach(var user in Model)
	{
		<tr>
			<td>@user.Email</td>
			<td>
				<a class="btn btn-primary" asp-action="Edit" asp-route-userid="@user.Id"><i class="bi bi-sliders2"></i> Roles Manager</a>
			</td>
		</tr>
	}
</table>